.marquee {
		width: 1000rpx;
		-webkit-animation: move 18s linear infinite;
		        animation: move 18s linear infinite;
		text-align: center;
}
@-webkit-keyframes move {
0% {
			-webkit-transform: translateX(1000rpx);
			        transform: translateX(1000rpx);
}

		/* 50%
	    {
	        transform:translateX(-1000upx);
	    } */
100% {
			-webkit-transform: translateX(-2600rpx);
			        transform: translateX(-2600rpx);
}

		/* from {left:200upx;}
		to {left:1000px;} */
}
@keyframes move {
0% {
			-webkit-transform: translateX(1000rpx);
			        transform: translateX(1000rpx);
}

		/* 50%
	    {
	        transform:translateX(-1000upx);
	    } */
100% {
			-webkit-transform: translateX(-2600rpx);
			        transform: translateX(-2600rpx);
}

		/* from {left:200upx;}
		to {left:1000px;} */
}
