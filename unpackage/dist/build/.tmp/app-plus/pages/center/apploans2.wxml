<view class="content"><view class="header"><view data-event-opts="{{[['tap',[['jump',['/pages/main/financial']]]]]}}" class="left_" bindtap="__e"><image src="../../static/icon/left_whitch.png" mode></image></view><view class="center_title"><text>申请贷款</text></view></view><view class="inslist"><block wx:for="{{inslist}}" wx:for-item="it" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['selins',[i]]]]]}}" class="inslist_it" bindtap="__e"><view class="inslist_selarch"><image src="{{it.operate==true?'../../static/icon/pitchon.png':'../../static/icon/nopitchon.png'}}" mode></image></view><view class="inslist_name"><text>{{it.item}}</text></view></view></block></view><view class="text_box"><view class="text_name"><text>贷款金额</text></view><input type="number" placeholder="请输入贷款金额" placeholder-class="inplss" data-event-opts="{{[['input',[['__set_model',['','money','$event',[]]]]]]}}" value="{{money}}" bindinput="__e"/></view><view class="text_box"><view class="text_name"><text>联系电话</text></view><input type="number" maxlength="11" placeholder="请输入联系电话" placeholder-class="inplss" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/></view><view class="text_box"><view class="text_name"><text>住址</text></view><input type="text" placeholder="现居住地址" placeholder-class="inplss" data-event-opts="{{[['input',[['__set_model',['','adress','$event',[]]]]]]}}" value="{{adress}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="bottom_btn" bindtap="__e"><text>确认申请贷款</text></view></view>