
.content {
	padding-left: 24rpx;
	padding-right: 24rpx;
	background: transparent;
}
.top_back {
	position: fixed;
	top: 0;
	left: 0;
	background: -webkit-gradient(linear, left top, left bottom, from(#2189FF), to(#f5f5f5));
	background: -o-linear-gradient(top, #2189FF, #f5f5f5);
	background: linear-gradient(to bottom, #2189FF, #f5f5f5);
	height: 772rpx;
	width: 100%;
	z-index: -1;
}
.top_back2 {
	position: fixed;
	top: 772rpx;
	left: 0;
	background: #EFF3F6;
	height: 100%;
	width: 100%;
	z-index: -1;
}
.record_box {
	margin: 30rpx auto 0;
	padding: 0 30rpx 30rpx;
	border-radius: 20rpx;
	text-align: center;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.re_num {
	color: #FFFFFF;
	font-size: 54rpx;
	margin-bottom: 20rpx;
}
.re_btn {
	color: #FFFFFF;
	font-size: 26rpx;
	background-color: #69AFFF;
	width: 180rpx;
	padding: 10rpx 20rpx;
	border-radius: 26rpx;
}
.integral_title{
	border-radius: 20rpx 20rpx 0 0;
	background-color: #FFFFFF;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	color: #3F3F3F;
	font-size: 26rpx;
	padding: 30rpx;
	border-bottom: 2rpx solid #F5F5F5;
}
.title_img{
	width: 30rpx;
	/* height: 30upx; */
	margin-right: 10rpx;
}
.inter_box{
	background-color: #FFFFFF;
	padding: 0rpx 30rpx 30rpx;
	border-radius: 0 0 20rpx 20rpx;
}
.inter_it{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	/* justify-content: center; */
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	padding: 30rpx  0;
	border-bottom: 2rpx solid #F5F5F5;
}
.inter_it:nth-last-of-type{
	border: none;
}
.sel_img{
	width: 10%;
	text-align: center;
}
.sel_img image{
	width: 32rpx;
	height: 32rpx;
}
.inter_text{
	width: 50%;
}
.inter_num{
	width: 40%;
	text-align: end;
}
.notice_box{
	margin-top: 30rpx;
	padding:0 30rpx 30rpx;
	background-color: #FFFFFF;
	border-radius: 20rpx;
}
.not_icon{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	padding: 30rpx;
}
.not_img{
	width: 36rpx;
	height: 40rpx;
	margin-right: 10rpx;
}
.not_title{
	color: #3F3F3F;
	font-size: 26rpx;
}
.noticwirte{
	height: 134rpx;
	padding: 30rpx;
	border: 2rpx solid #E4E4E4;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	margin: 0 auto ;
	border-radius: 20rpx;
	font-size: 24rpx;
}
.wirte_notice{
	color: #C7C6C6;
	font-size: 24rpx;
}
.loadclass{
	width: 100%;
	text-align: center;
	font-size: 24rpx;
	padding: 10rpx;
}

