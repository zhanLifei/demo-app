<view class="content"><header-nav vue-id="1" Htitle="{{title}}" url_le="{{url_le}}" Hlicon="{{icon}}" bind:__l="__l"></header-nav><view class="tab"><view data-event-opts="{{[['tap',[['changeTabs',[0]]]]]}}" class="{{['inn',current==0?'active':'']}}" bindtap="__e">未提交</view><view data-event-opts="{{[['tap',[['changeTabs',[1]]]]]}}" class="{{['inn',current==1?'active':'']}}" bindtap="__e">审核中</view><view data-event-opts="{{[['tap',[['changeTabs',[2]]]]]}}" class="{{['inn',current==2?'active':'']}}" bindtap="__e">已通过</view><view data-event-opts="{{[['tap',[['changeTabs',[3]]]]]}}" class="{{['inn',current==3?'active':'']}}" bindtap="__e">驳回</view></view><view class="list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><block wx:if="{{item.status!==0}}"><view class="item"><view class="img_box"><block wx:for="{{list[index].apply_fields}}" wx:for-item="items" wx:for-index="indexs" wx:key="indexs"><image src="{{items}}" mode></image></block></view><view class="item_sta"><view class="sta"><block wx:if="{{item.status==1}}"><view class="status bg1">审核中</view></block><block wx:if="{{item.status==2}}"><view class="status bg2">已通过</view></block><block wx:if="{{item.status==3}}"><view class="status bg3">驳回</view></block><view class="tim">{{''+item.createtime+''}}</view></view></view></view></block><block wx:else><view><view class="item"><block wx:for="{{list[index].apply_fields}}" wx:for-item="items" wx:for-index="indexs" wx:key="indexs"><view class="tip_"><text class="tip_title">{{items.set_name+":"}}</text><text class="tip_word">{{items.data+items.data}}</text></view></block><view class="item_sta"><view class="sta"><view data-event-opts="{{[['tap',[['jump',['/pages/service/up_post?id='+item.id]]]]]}}" class="status bg1" bindtap="__e">上传凭证</view><view class="tim">{{''+item.createtime+''}}</view></view></view></view></view></block></view></block></view></view>