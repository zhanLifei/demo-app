<view class="content"><headers vue-id="1" Htitle="{{Htitle}}" Hlicon="{{Hlicon}}" bind:__l="__l"></headers><view class="nav_box"><block wx:for="{{navlist}}" wx:for-item="it" wx:for-index="i" wx:key="id"><view data-event-opts="{{[['tap',[['selnav',['$0'],[[['navlist','id',it.id,'id']]]]]]]}}" class="{{['nav_it',[(it.id===index)?'active':'']]}}" bindtap="__e"><text>{{it.title}}</text></view></block></view><block wx:for="{{appoinlist}}" wx:for-item="it" wx:for-index="i" wx:key="i"><block wx:if="{{index===1}}"><view class="com_box"><view class="com_top"><view class="left_top"><image src="{{it.user.avatar}}" mode></image></view><view class="left_mid"><text>{{it.user.account}}</text></view><view class="right_time"><text style="margin-right:4rpx;">预约时间:</text><text>{{it.subscribe_time}}</text></view></view><view class="com_bot"><view data-event-opts="{{[['tap',[['cancelConsent',['$0',1],[[['appoinlist','',i,'id']]]]]]]}}" class="bot_btn1" bindtap="__e"><text>取消</text></view><view data-event-opts="{{[['tap',[['cancelConsent',['$0',2],[[['appoinlist','',i,'id']]]]]]]}}" class="bot_btn" bindtap="__e"><text>同意</text></view></view></view></block></block><block wx:for="{{appoinlist}}" wx:for-item="it" wx:for-index="i" wx:key="id"><block wx:if="{{index===2}}"><view class="com_box2"><view class="com_left"><view class="left_top2"><image src="{{it.user.avatar}}" mode></image></view><view class="left_mid2"><text>{{it.user.account}}</text></view></view><view class="com_right"><view class="right_text"><block wx:if="{{it.status==2}}"><text>同意</text></block><block wx:else><text>取消</text></block></view><view class="right_time2"><text style="margin-right:4rpx;">预约时间:</text><text>{{it.subscribe_time}}</text></view></view></view></block></block><uni-load-more vue-id="2" status="{{status}}" bind:__l="__l"></uni-load-more></view>