<view class="content"><headers vue-id="1" Htitle="{{Htitle}}" Hlicon="{{Hlicon}}" bind:__l="__l"></headers><view class="nav_box"><block wx:for="{{navlist}}" wx:for-item="it" wx:for-index="i" wx:key="id"><view data-event-opts="{{[['tap',[['selnav',['$0'],[[['navlist','id',it.id,'id']]]]]]]}}" class="{{['nav_it',[(it.id===index)?'active':'']]}}" bindtap="__e"><text>{{it.title}}</text></view></block></view><block wx:for="{{comlist}}" wx:for-item="it" wx:for-index="i" wx:key="id"><view class="com_box"><view class="com_left"><view class="left_top"><view class="top_title"><text>{{it.title}}</text></view><view class="top_num"><text>{{it.total_commision+" 张"}}</text></view></view><view class="left_bot"><text>{{it.create_time}}</text></view></view><view class="com_right"><block wx:if="{{it.status==1}}"><view class="right_btn"><text>待审核</text></view></block><block wx:else><block wx:if="{{it.status==2}}"><view class="right_btn2"><text>已通过</text></view></block><block wx:else><view class="right_btn2"><text>被驳回</text></view></block></block></view></view></block><uni-load-more vue-id="2" status="{{status}}" bind:__l="__l"></uni-load-more></view>