.imgbox {
  width: 710rpx;
  margin: 36rpx auto 0;
  background-color: #fff;
  padding: 32rpx 20rpx 40rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border-radius: 10rpx;
}
.imgbox .tit {
  font-size: 28rpx;
  color: #666;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  margin-bottom: 36rpx;
}
.imgbox .imgs {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.imgbox .imgs .imgs_box {
  position: relative;
}
.imgbox .imgs .imgs_box .close {
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  width: 80rpx;
  height: 80rpx;
}
.imgbox .imgs image {
  width: 140rpx;
  height: 140rpx;
}
